<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.east.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1">
        <activity android:name=".swap.SwapActivity" />
        <activity android:name=".itemanimator.ItemAnimatorActivity" />
        <activity
            android:name=".itemdecoration.ItemDecorationActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".recycleranalysis.RecyclerAnalysisActivity" />
        <activity
            android:name=".recycleranalysis.basicUse.BasicUseActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".recycleranalysis.commonAdapterUse.CommonAdapterActivity" />
        <activity android:name=".recycleranalysis.dragItemAnimatorUse.DragItemAnimatorActivity" />
        <activity android:name=".recycleranalysis.headerFooterUse.HeaderFooterActivity" />
        <activity android:name=".recycleranalysis.refreshLoad.RefreshLoadActivity" />
    </application>

</manifest>